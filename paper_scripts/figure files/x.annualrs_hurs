reinit
set display color white
set mproj robinson
c

open annualrs_hurs_historical.ctl
open annualrs_hurs_ssp126.ctl
open annualrs_hurs_ssp245.ctl
open annualrs_hurs_ssp370.ctl
open annualrs_hurs_ssp434.ctl
open annualrs_hurs_ssp585.ctl

set grid off
set lon -180 180
set lat -90 90
set grads off
set gxout shaded
set ylopts 1 4 0.15
set xlopts 1 4 0.15
exec y.rovers
d var.1
y.cbarn.gs
printim annualrs_hurs_historical.png
c


